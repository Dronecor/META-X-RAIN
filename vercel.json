{
    "version": 2,
    "builds": [
        {
            "src": "backend/main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "backend/main.py"
        }
    ],
    "env": {
        "PROJECT_NAME": "Agentic AI CRM",
        "SECRET_KEY": "set_in_vercel",
        "DATABASE_URL": "set_in_vercel",
        "GROQ_API_KEY": "set_in_vercel",
        "GROQ_MODEL": "llama-3.3-70b-versatile",
        "TWILIO_ACCOUNT_SID": "set_in_vercel",
        "TWILIO_AUTH_TOKEN": "set_in_vercel",
        "TWILIO_WHATSAPP_NUMBER": "set_in_vercel",
        "SENDGRID_API_KEY": "set_in_vercel",
        "PINECONE_API_KEY": "set_in_vercel",
        "PINECONE_ENV": "set_in_vercel",
        "SERPAPI_API_KEY": "set_in_vercel",
        "ADMIN_EMAIL": "set_in_vercel",
        "ADMIN_PASSWORD": "set_in_vercel"
    },
    "regions": [
        "iad1"
    ]
}